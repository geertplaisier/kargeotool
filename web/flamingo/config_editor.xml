        <fmc:Container borderwidth="0" bordercolor="#606060" left="0" top="33" height="776" width="82%">
            <fmc:BorderNavigation listento="map" top="0%" width="100%" height="100%" skin="f1"/>
            <tpc:EditMap id="drawMap" listento="map" left="0" top="0%" height="100%" width="100%" bottom="bottom">
                <tpc:GIS>
                    <tpc:Layer id="drawLay" name="drawLay" title="Teken KAR punt" visible="true" geometrytypes="Point" wfsurl="wfs::http://public-wms.kaartenbalie.nl/wms/transmodel_connexxion_edit?" version="1.0.0" featuretypename="kar_punten" loadfeaturesonstart="false">
                        <!--tpc:Layer id="drawLay" name="drawLay" title="Teken KAR punt" visible="true" geometrytypes="Point" wfsurl="wfs::http://b3p-demoserver/cgi-bin/mapserv.exe?map=e:/maps/transmodel_connexxion.map" version="1.0.0" featuretypename="kar_punten" loadfeaturesonstart="false"-->
                        <tpc:Style fillcolor="0x0000FF" fillopacity="30" strokecolor="0x0000FF" strokeopacity="50" strokeWidth="1"/>
                    </tpc:Layer>
                </tpc:GIS>
            </tpc:EditMap>
            <fmc:Map extenthistory="10" id="map" left="0" top="0%" height="100%" width="100%" bottom="bottom" visible="true" fullextent="12000,304000,280000,620000" extent="12000,304000,280000,620000">
				<!--fmc:LayerOGWMS id="bushaltes" url="http://b3p-demoserver/cgi-bin/mapserv.exe?map=e:/maps/transmodel_connexxion.map&amp;Service=WMS" srs="EPSG:28992" layers="bushaltes,bushaltes_labels,buslijnen,kar_punten" showerrors="true" query_layers="kar_punten,bushaltes"/-->
                <fmc:LayerOGWMS id="kar_punten" url="http://public-wms.kaartenbalie.nl/wms/transmodel_connexxion_edit?Service=WMS" srs="EPSG:28992" layers="buslijnen,bushaltes,bushaltes_labels,kar_punten" showerrors="true" query_layers="kar_punten,bushaltes"/>
                <fmc:LayerIdentifyIcon id="identifyicon"/>
                <fmc:LayerOGWMS id="osm" minscale="50" url="http://osm.kaartenbalie.nl/wms/mapserver-scale?Service=WMS" srs="EPSG:28992" layers="#ALL#" showerrors="true"/>
                <fmc:LayerOGWMS id="luchtfoto" maxscale="50" url="http://public-wms.kaartenbalie.nl/wms/aerogrid?Service=WMS" srs="EPSG:28992" layers="#ALL#" showerrors="true"/>
            </fmc:Map>
        </fmc:Container>
    </fmc:Container>
